
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://admin:password@localhost:5432/users?schema=public"
}

model User {
  id              String    @id @default(uuid())
  nome            String
  CPF             String    @unique
  email           String    @unique
  telefone        String
  sexo            String
  dataNascimento  DateTime

  @@map("usuarios")
}
